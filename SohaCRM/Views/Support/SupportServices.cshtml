                                                            var num_patt = /^[0-9]*$/;
                                                            var alphnumeric = /^[a-zA-Z ضصيكثقفغعهخحجچپشسیبلاتنمکگظطزرژءذدئوآ0123456789]*$/;
                                                            //console.log(alph_patt.test($('#CustomerConnector').val()));

                                                            if ($('#CustomerName').val() == "") {
                                                                ShowModal("مشتری انتخاب نشده است")
