                                                                ShowModal("لطفاً فیلد رابط را پر کنید")
                                                                return false;
                                                            }
                                                            else if (!alphnumeric.test($('#CustomerConnector').val())) {
                                                                ShowModal("فیلد نام رابط حاوی کاراکتر غیرمجاز است")
                                                                return false;
                                                            }
                                                            else if ($('#GuidanceDate').val() == "") {
                                                                ShowModal("لطفاً فیلد تاریخ پشتیبانی را پر کنید")
                                                                return false;
                                                            }
                                                            else if ($('#Problem').val() == "") {
                                                                ShowModal("لطفاً فیلد مشکل پیش آمده را پر کنید")
                                                                return false;
                                                            }
                                                            else if ($('#Guidance').val() == "") {
                                                                ShowModal("لطفاً فیلد راهنمایی انجام شده را پر کنید")
                                                                return false;
                                                            }




                                                            return true;
